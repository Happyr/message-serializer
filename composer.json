{
    "name": "happyr/message-serializer",
    "description": "Serialize classes the good way. ",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/messenger": "^4.4 || ^5.4 || ^6.0",
        "symfony/phpunit-bridge": "^4.4 || ^5.4 || ^6.0"
    },
    "suggest": {
        "symfony/messenger": "if you want to use a Symfony messenger integration"
    },
    "autoload": {
        "psr-4": {
            "Happyr\\MessageSerializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Happyr\\MessageSerializer\\": "tests/"
        }
    }
}
